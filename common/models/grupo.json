{
  "name": "Grupo",
  "plural": "Grupos",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "Nombre": {
      "type": "string",
      "required": true
    },
    "Descripcion": {
      "type": "string",
      "required": true
    }
  },
  "validations": [],
  "relations": {
    "alumnos": {
      "type": "hasMany",
      "model": "Alumno",
      "foreignKey": "grupoId",
      "options": {
        "nestRemoting": true
      },
      "through": "Matricula"
    },
    "equipos": {
      "type": "hasMany",
      "model": "Equipo",
      "foreignKey": "grupoId"
    },
    "juegos": {
      "type": "hasMany",
      "model": "Juego",
      "foreignKey": "grupoId"
    }
  },
  "acls": [],
  "methods": {}
}

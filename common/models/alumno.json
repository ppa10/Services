{
  "name": "Alumno",
  "plural": "Alumnos",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "Nombre": {
      "type": "string",
      "required": true
    },
    "PrimerApellido": {
      "type": "string",
      "required": true
    },
    "SegundoApellido": {
      "type": "string",
      "required": true
    },
    "ImagenPerfil": {
      "type": "string",
      "required": true,
      "default": "fotoAlumno"
    }
  },
  "validations": [],
  "relations": {
    "grupos": {
      "type": "hasMany",
      "model": "Grupo",
      "foreignKey": "alumnoId",
      "options": {
        "nestRemoting": true
      },
      "through": "Matricula"
    }
  },
  "acls": [],
  "methods": {}
}

{
  "name": "Alumno",
  "plural": "Alumnos",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "Nombre": {
      "type": "string",
      "required": true
    },
    "Apellido": {
      "type": "string",
      "required": true
    },
    "ImagenPerfil": {
      "type": "string",
      "required": true,
      "default": "fotoAlumno"
    }
  },
  "validations": [],
  "relations": {
    "grupos": {
      "type": "hasAndBelongsToMany",
      "model": "Grupo",
      "foreignKey": "alumnoId"
    },
    "equipos": {
      "type": "hasAndBelongsToMany",
      "model": "Equipo",
      "foreignKey": "equipoId"
    },
    "coleccions": {
      "type": "hasAndBelongsToMany",
      "model": "Coleccion",
      "foreignKey": "coleccionId"
    },
    "albums": {
      "type": "hasMany",
      "model": "Album",
      "foreignKey": "albumId"
    }
  },
  "acls": [],
  "methods": {}
}

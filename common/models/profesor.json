{
  "name": "Profesor",
  "plural": "Profesores",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "Nombre": {
      "type": "string",
      "required": true
    },
    "Apellido": {
      "type": "string",
      "required": true
    },
    "ImagenPerfil": {
      "type": "string",
      "required": true,
      "default": "foto"
    }
  },
  "validations": [],
  "relations": {
    "grupos": {
      "type": "hasMany",
      "model": "Grupo",
      "foreignKey": "profesorId"
    },
    "alumnos": {
      "type": "hasMany",
      "model": "Alumno",
      "foreignKey": "profesorId"
    }
  },
  "acls": [],
  "methods": {}
}
